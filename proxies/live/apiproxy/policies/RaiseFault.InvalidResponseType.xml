<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseFault.InvalidResponseType">
  <FaultResponse>
    <Set>
      <Headers>
        <Header name="Location">{private.apigee.authorize_endpoint}?response_type={request.queryparam.response_type}&amp;client_id={private.apigee.client_id}&amp;redirect_uri={private.apigee.redirect_uri}&amp;scope=openid&amp;state={apigee.state}&amp;error=unsupported_response_type&amp;error_description=Response type is not supported&amp;message_id={messageid}</Header>
      </Headers>
      <StatusCode>302</StatusCode>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
