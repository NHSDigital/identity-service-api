<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.SetAuthorizationRedirectCis2">
  <Set>
    <StatusCode>302</StatusCode>
    <Headers>
      <Header name="Location">https://internal-dev.api.service.nhs.uk/mock-nhsid-jwks-pr-46/simulated_auth?response_type={request.queryparam.response_type}&amp;client_id={identity-service-config.cis2.client_id}&amp;redirect_uri=https://internal-dev.api.service.nhs.uk/oauth2-pr-235/callback&amp;scope=openid%20nationalrbacaccess&amp;state={apigee.state}&amp;max_age=300</Header>
    </Headers>
  </Set>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="response" />
</AssignMessage>
