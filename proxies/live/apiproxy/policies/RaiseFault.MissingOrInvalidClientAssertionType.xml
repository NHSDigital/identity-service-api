<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseFault.MissingOrInvalidClientAssertionType">
  <FaultResponse>
    <Set>
      <Payload contentType="application/json">
            {
              "error": "invalid_request",
              "error_description": "Missing or invalid client_assertion_type - must be 'urn:ietf:params:oauth:client-assertion-type:jwt-bearer'",
              "message_id": "{messageid}"
            }
</Payload>
      <StatusCode>400</StatusCode>
      <ReasonPhrase>Bad Request</ReasonPhrase>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
