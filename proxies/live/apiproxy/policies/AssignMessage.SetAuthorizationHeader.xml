<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.SetAuthorizationHeader">
  <Set>
    <Headers>
      <Header name="Authorization">{accesstoken.external_access_token}</Header>
    </Headers>
  </Set>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>
