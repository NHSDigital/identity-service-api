<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<HMAC name="HMAC.HashAccessTokenClientCredentials">
    <DisplayName>HMAC.HashAccessToken</DisplayName>
    <Algorithm>SHA-512</Algorithm>
    <Message ref="oauthv2accesstoken.OAuthV2.ClientCredentialsGenerateAccessTokenFilteredScopes"/>
    <SecretKey ref="access_token.hash_secret"/>
    <Output>auth.access_token_hash</Output>
</HMAC>
