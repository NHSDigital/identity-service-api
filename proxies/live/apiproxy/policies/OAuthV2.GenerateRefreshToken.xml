<OAuthV2 name="OAuthV2.GenerateRefreshToken">
  <Operation>RefreshAccessToken</Operation>
  <GenerateResponse enabled="false"/>
  <!-- 10 minutes: -->
  <ExpiresIn ref="private.apigee.access_token_expiry_ms">600000</ExpiresIn>
  <!-- 1 hour: -->
  <RefreshTokenExpiresIn ref="private.apigee.refresh_token_expiry_ms">3600000</RefreshTokenExpiresIn>
</OAuthV2>
