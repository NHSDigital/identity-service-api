<OAuthV2 name="OAuthV2.GenerateRefreshToken">
  <Operation>RefreshAccessToken</Operation>
  <GenerateResponse enabled="false"/>
  <!-- 10 minutes: -->
  <ExpiresIn ref="identity-service-config.cis2.access_token_expiry_ms">600000</ExpiresIn>
  <!-- 12 hours: -->
  <RefreshTokenExpiresIn ref="identity-service-config.cis2.refresh_token_combined_expiry_ms">43200000</RefreshTokenExpiresIn>
</OAuthV2>
