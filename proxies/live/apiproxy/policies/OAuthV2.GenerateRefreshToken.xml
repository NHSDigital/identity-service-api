<OAuthV2 name="OAuthV2.GenerateRefreshToken">
  <Operation>RefreshAccessToken</Operation>
  <GenerateResponse enabled="false"/>
  <!-- default if ref cannot be found is set at 10 minutes -->
  <ExpiresIn ref="apigee.access_token_expiry_ms">600000</ExpiresIn>
  <!--  default if ref cannot be found is set at 1 hour (minimum expected) -->
  <RefreshTokenExpiresIn ref="apigee.refresh_token_expires_in_ms">3600000</RefreshTokenExpiresIn>
</OAuthV2>
