<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseFault.InvalidSubjectTokenType">
    <DisplayName>RaiseFault.InvalidSubjectTokenType</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json">
        {
          "error": "invalid_request",
          "error_description": "missing or invalid subject_token_type - must be 'urn:ietf:params:oauth:token-type:id_token'",
          "message_id": "{messageid}"
        }
</Payload>
            <StatusCode>400</StatusCode>
            <ReasonPhrase>Bad Request</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>