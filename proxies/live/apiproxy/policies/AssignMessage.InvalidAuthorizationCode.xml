<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.InvalidAuthorizationCode">
    <DisplayName>Invalid auth code</DisplayName>
    <Set>
      <Payload contentType="application/json">
        {
          "error" : "invalid_grant",
          "error_description" :"code is invalid",
          "message_id": "{messageid}"
        }
      </Payload>
    </Set>
    <AssignTo createNew="false" transport="https" type="request"/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>
