<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.GenerateTokenResponseSimulatedAuth">
  <Set>
    <Payload contentType="application/json">
      {
        "access_token": "{oauthv2accesstoken.OAuthV2.GenerateAccessTokenSimulatedAuthFilteredScopes.access_token}",
        "expires_in": "{oauthv2accesstoken.OAuthV2.GenerateAccessTokenSimulatedAuthFilteredScopes.expires_in}",
        "refresh_token": "{oauthv2accesstoken.OAuthV2.GenerateAccessTokenSimulatedAuthFilteredScopes.refresh_token}",
        "refresh_token_expires_in": "{oauthv2accesstoken.OAuthV2.GenerateAccessTokenSimulatedAuthFilteredScopes.refresh_token_expires_in}",
        "refresh_count": "{oauthv2accesstoken.OAuthV2.GenerateAccessTokenSimulatedAuthFilteredScopes.refresh_count}",
        "token_type": "Bearer"
      }
    </Payload>
  </Set>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
<AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
